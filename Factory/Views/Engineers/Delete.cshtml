@{
  Layout = "_Layout";
}

@model Factory.Models.Engineer
<h2>Delete Engineer</h2>
<hr />
<h3>Please confirm removal of "@Html.DisplayFor(model => model.EngineerName)" and all repair licenses.</h3><br>

@using (Html.BeginForm())
{
  <input type="submit" value="CONFIRM" />
}
<hr />
<h5>@Html.ActionLink($"Back to {Model.EngineerName}'s Details", "Details", new { id = Model.EngineerId })</h5>