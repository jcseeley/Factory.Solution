@{
  Layout = "_Layout";
}

@model Factory.Models.Engineer
<h1>Delete Engineer</h1>
<hr />
<h2>Please confirm removal of "@Html.DisplayFor(model => model.EngineerName)" and all repair licenses.</h2><br>

@using (Html.BeginForm())
{
  <input type="submit" value="CONFIRM" />
}
<hr />
<h4>@Html.ActionLink($"Back to {Model.EngineerName}'s Details", "Details", new { id = Model.EngineerId })</h4>